{
	"name": "nextjs-apollo-typescript-starter",
	"version": "1.0.0",
	"description": "Next.js 9 with Apollo GraphQL and TypeScript",
	"main": "index.js",
	"author": "jpwesselink",
	"license": "MIT",
	"scripts": {
		"dev": "stmux -M -e ERROR -m beep,system -- [ [ \"next\" .. \"yarn generate-graphql-types -w\" ] ] && pkill -f next",
		"generate-graphql-types": "graphql-codegen -c graphql-code-gen.yml",
		"build": "next build",
		"start": "next start",
		"lint": "eslint lib pages",
		"lint-fix": "eslint -c .eslintrc.js lib pages --fix"
	},
	"dependencies": {
		"apollo-boost": "^0.4.3",
		"apollo-client": "^2.6.3",
		"apollo-server-micro": "^2.6.9",
		"graphql": "^14.4.2",
		"graphql-import": "^0.7.1",
		"graphql-tag": "^2.10.1",
		"isomorphic-unfetch": "^3.0.0",
		"next": "^9.0.1",
		"react": "^16.8.6",
		"react-apollo": "^2.5.8",
		"react-dom": "^16.8.6"
	},
	"devDependencies": {
		"@graphql-codegen/add": "^1.4.0",
		"@graphql-codegen/cli": "^1.4.0",
		"@graphql-codegen/typescript-operations": "^1.4.0",
		"@graphql-codegen/typescript-react-apollo": "^1.4.0",
		"@graphql-codegen/typescript-resolvers": "^1.4.0",
		"@types/graphql": "^14.2.2",
		"@types/node": "^12.6.2",
		"@types/react": "^16.8.23",
		"@types/react-dom": "^16.8.4",
		"@typescript-eslint/eslint-plugin": "^1.12.0",
		"@typescript-eslint/parser": "^1.12.0",
		"babel-eslint": "^10.0.2",
		"eslint": "^5.16.0",
		"eslint-config-airbnb": "^17.1.1",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-import": "^2.18.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-react": "^7.14.2",
		"prettier": "^1.18.2",
		"stmux": "^1.7.1",
		"typescript": "^3.5.3"
	}
}
